pcm.jack_aux_live {
    type jack
    playback_ports {
        0 system:playback_1
    }
    capture_ports {
        0 system:capture_1
    }
}

jack_aux_live {
    type plug
    slave { pcm "pcm.jack_aux_live" }
}

pcm.jack_aux {
    type jack
    playback_ports {
        0 system:playback_1
    }
    capture_ports {
        0 system:capture_1
    }
}

jack_aux {
    type plug
    slave { pcm "pcm.jack_aux" }
}
